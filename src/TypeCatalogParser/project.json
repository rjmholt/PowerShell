{
    "buildOptions": {
        "warningsAsErrors": true,
        "emitEntryPoint": true
    },

    "dependencies": {
        "Microsoft.NETCore.App": "1.1.0",
        "Microsoft.DotNet.ProjectModel": "1.0.0-rc3-1-003177",
        "Microsoft.DotNet.Cli.Utils": "1.0.0-preview3-004056"
    },

    "configurations": {
        "Linux": {
            "buildOptions": {
                "define": [ "UNIX" ],
                "debugType": "portable"
            }
        },
        "Debug": {
            "buildOptions": {
                "debugType": "portable"
            }
        },
        "Release": {
            "buildOptions": {
                "debugType": "portable"
            }
        },
        "CodeCoverage": {
            "buildOptions": {
                "debugType": "full"
            }
        }
    },

    "frameworks": {
        "netcoreapp1.1": {
            "imports": [ "dnxcore50", "portable-net45+win8" ]
        }
    },

    "runtimes": {
        "ubuntu.14.04-x64": { },
        "ubuntu.16.04-x64": { },
        "ubuntu.16.10-x64": { },
        "centos.7-x64": { },
        "win7-x86": { },
        "win7-x64": { },
        "win10-x64": { },
        "osx.10.11-x64": { }
    }
}
